mysql -u root -p                           %en local (ssh ?)
mysql -h hostname -u username -p           %where hostname is the host name of the MySQL server

create database tests;

use tests;

create table fichiers_entiers ( id int not null auto_increment primary key, le_latex text, le_pdf mediumblob, le_type char(5) not null, l_auteur text not null, l_annee char(11) not null) default character set utf8;

