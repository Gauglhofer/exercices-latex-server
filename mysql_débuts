mysql -u root -p                           //en local (ssh ?)
mysql -h hostname -u username -p           //where hostname is the host name of the MySQL server

CREATE DATABASE tests;

USE tests;

CREATE TABLE fichiers_entiers ( id INT NOT NULL AUTO_INCREMENT PRIMARY KEY, le_nom TEXT NOT NULL, le_type CHAR(5) NOT NULL, l_auteur TEXT NOT NULL, l_annee YEAR(4) NOT NULL, latex TINYINT NOT NULL) DEFAULT CHARACTER SET utf8;


